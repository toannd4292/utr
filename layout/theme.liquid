                      name: 'consent-tracking-api',
                      version: '0.1',
                  },
              ],
              error => {
                  if (error) {
                      throw error; 
                  }
  
                  sendConsent(trackingConsent)
  
              },
          );   
      }
  
      (function () {
          waitForOneTrust();
      })();
  
