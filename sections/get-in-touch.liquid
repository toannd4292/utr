const formatPhoneNumber = (phoneNumber, countryCode) => {
  let formattedPhoneNumber = phoneNumber.replace(/\D/g, "");
  if (countryCode.length === 1) {
    formattedPhoneNumber = `+${countryCode.substr(0, 1)} ${formattedPhoneNumber.substr(1, 4)} ${formattedPhoneNumber.substr(4, 7)} ${formattedPhoneNumber.substr(10)}`;
  } else if (countryCode.length === 2) {
    formattedPhoneNumber = `+${countryCode.substr(0, 2)} ${formattedPhoneNumber.substr(3, 7)} ${formattedPhoneNumber.substr(8,10)}`;
    console.log(formattedPhoneNumber)
  } else if (countryCode.length >= 3 ) {
    formattedPhoneNumber = `+${countryCode.substr(0, 3)} ${formattedPhoneNumber.substr(3, 10)}`;
  }
  return formattedPhoneNumber;
};

