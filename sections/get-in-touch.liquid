 .flagdropdown dd, .flagdropdown dt,  dl.flagdropdown, .flagdropdown ul {
	margin: 0px;
	padding: 0px;
}
.flagdropdown dd ul li a span:first-child, .flagdropdown dt a span span:first-child {
	background-image: url('https://i.imgur.com/OQiDoZe.png');
	background-repeat: no-repeat;
	width: 16px;
	height: 11px;
	display: inline-block;
	margin-right:5px;
}
.flagdropdown dt a span {
  cursor: pointer;
  display: flex;
  justify-content: center;
  align-items: center;
  color:var(--color-black);
  font-size:14px;
  padding: 5px 0;
  white-space: nowrap;
}
.flagdropdown dt a img {
	position: relative;
	z-index: 1;
}
.flagdropdown dt a span span:first-child:before {
	position: absolute;
	content: '';
	width: 15px;
	height: 10px;
	box-shadow: 0 1px 1px rgba(0,0,0,0.2) inset;
}
.flagdropdown dt a span span {
	display: inline-block;
	padding: 0;
}
.flagdropdown dt a span span:first-child {
	padding: 0;
}
.flagdropdown dd {
	position: relative;
}
.flagdropdown a, .flagdropdown a:visited {
	color: #4a535f;
	text-decoration: none;
	outline: none;
}
.flagdropdown a:hover {
	color: #5d4617;
}
.flagdropdown dt a:hover, .flagdropdown dt a:focus {
	color: var(--color-black);
}
.flagdropdown dt a {
	position: relative;
	display: block;
	padding-right: 26px;
    font-family: "gotham-light";
}
.flag-b-right{position: relative;}  
.flag-b-right:after{
    content: "";
    position: absolute;
    right: 0px;
    top: 0px;
    background: var(--color-black);
    height: 24px;
    width: 1px;
    opacity:0.3
}
.flagdropdown dt a:after{
    content: "";
    position: absolute;
    right: 10px;
    top: 8px;
    border-top: solid 6px var(--color-black);
    border-left: solid 6px transparent;
    border-right: solid 6px transparent;
    border-radius: 2px;
}
.flagdropdown dd ul {
	background: var(--color-white);
	color: var(--color-black);
	display: none;
	left: 0px;
	padding: 5px 0px;
	position: absolute;
	width: 270px;
	list-style: none;
	max-height: 170px;
	overflow-y: scroll;
	top:10px;
	z-index: 2;
}
.flagdropdown span.value {display: none;}
.flagdropdown dd ul li a {
	padding: 5px;
	display: block;
	font-size: 14px !important;
    color:var(--color-black);
}
.flagdropdown dd ul li a:hover{background-color: rgba(0,0,0,0.05);}
dl.flagdropdown > span:nth-child(2) {
	overflow: hidden;
	white-space: nowrap;
	display: inline-block;
}
dl.flagdropdown span:nth-child(3) {float: right;}
dl.flagdropdown dt span:nth-child(2),
dl.flagdropdown dt span:nth-child(3){color:#fff;display: none;}
.countryFlag {
	padding: 0;
	background-image: url("https://i.imgur.com/OQiDoZe.png");
	background-repeat: no-repeat;
	display: inline-block;
	height: 11px;
	margin-right: 4px;
	width: 16px;
	cursor: pointer;
	white-space: nowrap;
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	-moz-border-right-colors: none;
	-moz-border-top-colors: none;
	border-color: #BFBFC1 #B6B6B6 #969696;
	border-image: none;
	border-radius: 2px 2px 2px 2px;
	border-style: solid;
	border-width: 1px;
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.09);
}

   /* popup style */
        #overlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5); /* Semi-transparent black background */
            z-index: 999; /* Ensure the overlay appears behind the popup */
        }

        /* Popup style */
        #mailuspopup {
            background: var(--color-gray);
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
            border-radius: 16px;
            z-index: 1000; /* Ensure the popup appears above the overlay */
            transition: transform 0.3s ease-in-out; /* Add a transition for smooth zoom-in effect */
        }
       #mailuspopup button.mfp-close {
         color: var(--body-bg);
