<div class="page-width text-center pt-lg-3 pt-sm-3">
  <div class="mb-2 mb-sm-1">
                    {% case section.settings['heading-size'] %}
      {% when 'h1' %}
        <h1 class="text-uppercase section-title section-title-large mb-0">
          
          {{ section.settings.heading }}
        </h1>
      {% when 'h2' %}
        <h2 class="text-uppercase section-title section-title-medium mb-0">
          
          {{ section.settings.heading }}
        </h2>
      {% when 'h3' %}
        <h3 class="text-uppercase section-title section-title-small mb-0">
          
          {{ section.settings.heading }}
        </h3>
