{% schema %}
  {
    "name": "Ut Gradient Detail Card",
     "presets": [
      {
        "name": "Ut Gradient Detail Card",
        "category": "Ut Gradient Detail Card"
      }
    ],
    "settings": [
       {
			"type":"text",
			"id":"heading",
			"label": "Heading Title"
		},
        {
            "type": "radio",
            "id": "heading-permission",
            "label": "Do You Want Underline Below Heading?",
          "default": "NO",
            "options": [
        {
            "value": "NO",
            "label": "NO"
        },
        {
            "value": "YES",
            "label": "YES"
        }
            ]
        }
    ],
    "blocks":[
	  {
		"type":"home-card",
		"name":"Ut Gradient Detail block",
		"settings":[
        {
			"type":"text",
			"id":"heading",
			"label": "College Title"
		},
        {
			"type":"text",
			"id":"event",
			"label": "Event Place Name"
		},
        {
			"type":"text",
			"id":"location",
			"label": "Location"
		},
        {
			"type":"text",
			"id":"event-date",
			"label": "Event Date"
		},
        {
			"type":"text",
			"id":"event-month",
			"label": "Event Month"
		},
        {
            "type": "select",
            "id": "gender",
            "label": "Gender",
            "options": [
              {
                "value": "Male",
                "label": "Male"
              },
              {
                "value": "Female",
                "label": "Female"
              }
            ],
            "default": "Male"
        },
        {
 			"type": "text",
 			"id": "button-title",
 			"label": "Button Title"
 		},
		{
			"type": "url",
 			"id": "button-url",
 			"label": "Button Link"
		}
		]
	  }
    ]
  }
{% endschema %}

