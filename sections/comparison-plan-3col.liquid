.most-bg{position:relative}
.most-bg-wrap{
  position:absolute;
  padding: 0.5rem 0.5rem;
  font-family: var(--font-gotham-medium);
  left: 0;
  right: 0;
  text-align: center;
} 
.most-bg-wrap:after,
.most-bg-wrap:before{content: "";position: absolute;}  
@media(min-width:992px) {
  .most-bg-wrap{ 
      background: linear-gradient(94.95deg, #FFCF00 6.96%, #FFB800 97.11%);
      border-radius: 0px 0px 15px 15px;
      width: 160px;
      margin: -33px auto auto;
      font-size: 14px;
  } 
  .most-bg-wrap:before{
      right: -13px;
      top: 0;
      border-bottom: 13px solid #E0BD5C;
      border-right: 13px solid transparent;
  }
  .card-middle:active, .card-right:active, 
  .card-middle:visited, .card-right:visited, 
  .card-middle:hover, .card-right:hover {
    margin-top: -4rem;
    padding-top: 4rem;
    padding-bottom: 2rem;
  }
} 
@media(max-width:991px) {
   .most-bg-wrap{
    font-size: 8px;
    background: linear-gradient(94.95deg, #FFCF00 6.96%, #FFB800 97.11%);
    border-radius: 0px 0px 10px 10px;
    width: 68px;
    margin: -10px auto auto;
   }  
    .most-bg-wrap:before{
      right: -10px;
      top: 0;
      border-bottom: 10px solid var(--color-black);
      border-right: 10px solid transparent;
    }
} 
@media(min-width:320px) and (max-width:420px){  
  .price-card-mobile{padding: 1.25rem 0.35rem !important;}
