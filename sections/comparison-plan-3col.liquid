<script>
  $(function() {
  $('.itemtable').matchHeight();
});
$(".filter-list").each(function () {
  var liCount = $(this).children("ul").length;
  if (liCount > 5) {
    $(this).next(".more").addClass("showMe");
  }
});
$(".more").click(function () {
  $(this).prev(".filter-list").find("ul").toggleClass("showList");
  $(".more svg").toggleClass("rotate-180");
   var lable = $(".morebtn").text().trim();
   if(lable == "View Less") {
     $(".morebtn").text("View All Features");
     $(".myText").hide();
   }
   else {
     $(".morebtn").text("View Less");
     $(".myText").show();
   }
  });
  
$(".filter-list2 ul").each(function () {
  var liCount = $(this).children("li").length;
  if (liCount > 3) {
    $(this).next(".more2").addClass("showMe");
  }
});
$(".more2").click(function () {
   $(".more2 svg").toggleClass("rotate-180")
  $(this).prev("ul").find("li").toggleClass("showList");
   var lable = $(".morebtn2").text().trim();

   if(lable == "View Less") {
     $(".morebtn2").text("View All Features");
     $(".myText").hide();
   }
   else {
     $(".morebtn2").text("View Less");
     $(".myText").show();
   }
  });
  $('.data').hide()
  jQuery('.morebutton').on('click',function(){
    jQuery('.data').toggle();
  })
</script>