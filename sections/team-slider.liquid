<div class="page-width text-center pt-lg-0 pt-sm-3">
  <div class="text-center mb-4 mb-sm-1">
    <h2 class="text-uppercase section-title section-title-sm mb-0 lh-sm-40 mb-sm-2">{{section.settings.heading}}</h2>
    {%- if section.settings.heading-permission == "YES" -%}
    <div class="divider-sm-teal divider-lg-teal"></div>
    {%- endif -%}
  </div>
</div>
<div class="position-relative text-center pb-lg-5 pb-sm-3">
   <div class="slick-carousel-complex-variant-1 team-member overflow-hidden">
          <div id="child-carousel" data-for=".carousel-parent" data-arrows="true" data-loop="true" data-dots="false" data-swipe="true" data-items="1" data-xs-items="1" data-sm-items="1" data-md-items="3" data-lg-items="3" data-slide-to-scroll="1" data-center-mode="true" data-center-margin="0" class="slick-slider slick-slider-images slick-slider">
            {% for block in section.blocks %}   
              <div class="item">
                 <div class="teamimg border-radius-lg-3 border-radius-sm-3" style="background:url('{{block.settings.block-image |  img_url:'large'}}') no-repeat center center; background-size: cover;display:block;"></div>
              </div>
            {% endfor %}
          </div>
                <div data-arrows="false" data-loop="true" data-dots="true" data-swipe="true" data-child="#child-carousel" data-for="#child-carousel" data-items="1" data-xs-items="1" data-sm-items="1" data-md-items="1" data-lg-items="1" data-slide-to-scroll="1" data-center-mode="true" class="slick-slider carousel-parent slick-dots-variant-1 offset-top-40">
               {% for block in section.blocks %}      
                  <div class="item">
                    <div class="itemhides">
                      <div class="team-name mt-3 lh-xl-78 text-black mb-2 lh-sm-normal">{{block.settings.member}}</div>
                      <div class="divider-sm-teal divider-lg-teal"></div>
                      <div class="team-text text-black mt-2 fw-bold">{{block.settings.description}}</div>
                    </div>
                  </div>
                {% endfor %}  
                </div>
              </div>
</div>
{% schema %}
  {
    "name": "team Slider Section",
    "presets": [
      {
        "name": "team Slider Section",
        "category": "team Slider Section"
      }
    ],
    "settings": [
        {
      "type":"text",
      "id":"heading",
      "label": "Heading Title"
    },
      {
            "type": "radio",
            "id": "heading-permission",
            "label": "Do You Want Underline Below Heading?",
          "default": "NO",
            "options": [
        {
            "value": "NO",
            "label": "NO"
        },
        {
            "value": "YES",
            "label": "YES"
        }
            ]
        },
        {
      "type":"text",
      "id":"sub-heading",
      "label": "Sub Title"
    }
    ],
     "blocks":[
    {
    "type":"team-card",
    "name":"Team block",
    "settings":[
          {
            "type": "image_picker",
            "id": "block-image",
            "label": "Card Image"
         },
        {
      "type":"text",
      "id":"member",
      "label": "Member Name"
    },  
        {
      "type":"text",
      "id":"description",
      "label": "Description"
    }
    ]
    }
    ]
  }
{% endschema %}

