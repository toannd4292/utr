<style>
.slick-slide{margin-left:8px; margin-right:8px;}
.slick-slide.slick-active.slick-center {transition: all 0.4s ease-in-out;}
.team-member .teamimg{transition: all 0.5s; cursor: pointer;}
.slick-carousel-complex-variant-1 .slick-next:before {
    background:url('https://cdn.shopify.com/s/files/1/0690/8071/1488/files/arrow-right_cd5e75b6-5dd5-4c8a-bfa8-2925d0930f72.svg?v=1675748941') no-repeat top center;
    width:40px;
    height:40px;  
}
.slick-carousel-complex-variant-1 .slick-prev:before {
    background:url('https://cdn.shopify.com/s/files/1/0690/8071/1488/files/arrow-left_ca496818-8957-48f5-afb5-dac64c36737e.svg?v=1675748941') no-repeat top center;
    width:40px;
    height:40px;  
}
.slick-carousel-complex-variant-1 .slick-prev:before, 
.slick-carousel-complex-variant-1 .slick-next:before {
    font-size: 40px;
    opacity: 100;
    color: transparent;
}
.slick-initialized .slick-slide .itemhides{display:none !important; overflow:hidden}
.slick-slide.slick-active.slick-center .itemhides{display:block !important;}
@media(max-width:991px){
  .team-member .teamimg{padding-bottom:100%;} 
  .team-member .teamimg{margin-top:2.5rem;}
  .slick-slide.slick-active.slick-center .teamimg {filter: grayscale(0%);margin-top: 5px;}
  .slick-slide.slick-active.slick-center .teamimg{padding-bottom:120%;}
  .slick-slide.slick-active.slick-center .itemhides{margin-left:-30%; margin-right:-30%;}
  .slick-carousel-complex-variant-1 .slick-prev, 
  .slick-carousel-complex-variant-1 .slick-next{top:auto; bottom:-80px}
  .slick-carousel-complex-variant-1 .slick-prev, 
  .slick-carousel-complex-variant-1 .slick-next{width:42px; height:58px; z-index:9;}
  .slick-carousel-complex-variant-1 .slick-next {right: 37%;}
  .slick-carousel-complex-variant-1 .slick-prev {left: 37%;}
  .itemhides{padding-top:6rem}
}
 @media(min-width:992px){
  .team-member .teamimg{/*height: 330px;*/ padding-bottom:100%} 
  .team-member .teamimg{margin-top:2.5rem;}
  .slick-slide.slick-active.slick-center .teamimg {padding-bottom:120%;filter: grayscale(0%);margin-top: 0;}
  .slick-carousel-complex-variant-1 .slick-prev, 
  .slick-carousel-complex-variant-1 .slick-next{width:40px; height:40px; z-index:1;}
  .slick-carousel-complex-variant-1 .slick-next {right: 24px;}
  .slick-carousel-complex-variant-1 .slick-prev {left: 24px;}
}
.teamimg:hover {filter: grayscale(0%);}
@media(min-width:1200px){
  .team-member .teamimg{margin-top:3.5rem;}
  .team-member .teamimg{/*height: 330px;*/ padding-bottom:100%} 
  .slick-slide.slick-active.slick-center .teamimg {padding-bottom:120%;filter: grayscale(0%);margin-top: 0;}
}  
@media(min-width:1800px){
  .team-member .teamimg{margin-top:6.5rem;}
}  
</style>
{{ 'slick.css' | asset_url | stylesheet_tag }}
